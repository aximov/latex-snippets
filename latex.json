{
/*
	 // Place your snippets for LaTeX here. Each snippet is defined under a snippet name and has a prefix, body and 
	 // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	 // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	 // same ids are connected.
	 // Example:
	 "Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	}
*/
"scope": {
		"prefix": "scope",
		"body": [
			"\\( $1 \\)"
		],
		"description": "MathJax scope"
	},
"mathrm": {
		"prefix": "mathrm",
		"body": [
			"\\mathrm{ $1 }$0"
		],
		"description": "mathrm"
	},
"unit symbol": {
		"prefix": "symbol",
		"body": [
			"\\, \\mathrm{ $1 }$0"
		],
		"description": "symbol"
	},
"quantity": {
		"prefix": "quantity",
		"body": [
			"\\( $1 \\, \\mathrm{ $2 } \\)$0"
		],
		"description": "MathJax Quantity"
	},
"letter": {
		"prefix": "letter",
		"body": [
			"\\( \\mathrm{ $1 } \\)$0"
		],
		"description": "MathJax letter"
	},
"A": {
		"prefix": "a-letter",
		"body": [
			"\\( \\mathrm{ A } \\)$0"
		],
		"description": "MathJax letter A"
	},
"B": {
		"prefix": "b-letter",
		"body": [
			"\\( \\mathrm{ B } \\)$0"
		],
		"description": "MathJax letter B"
	},
"ana": {
		"prefix": "ana",
		"body": [
			"[　$1　]$0"
		],
		"description": "MathJax 穴埋めの穴"
	},
"dfraction": {
		"prefix": "dfra",
		"body": [
			"\\displaystyle \\frac {$1} {$2}$0"
		],
		"description": "MathJax 分数"
	}
}
